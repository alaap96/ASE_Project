<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.vasireddyalaap.twinkle_world.LocationUpdate">

    <TextView
        android:id="@+id/Prev_TS_Label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/Pres_TS_Label"
        android:layout_marginTop="118dp"
        android:text="Prevs TS"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="45dp" />

    <TextView
        android:id="@+id/Pres_TS_Label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView5"
        android:layout_below="@+id/Prev_TS_Label"
        android:layout_marginTop="82dp"
        android:text="Pres TS"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="109dp" />

    <TextView
        android:id="@+id/Prev_TS_Value"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteY="45dp"
        tools:layout_editor_absoluteX="125dp"
        android:layout_alignBaseline="@+id/Prev_TS_Label"
        android:layout_alignBottom="@+id/Prev_TS_Label"
        android:layout_toEndOf="@+id/Prev_TS_Label"
        android:layout_marginStart="22dp" />

    <TextView
        android:id="@+id/Pres_TS_Value"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/Pres_TS_Label"
        android:layout_alignBottom="@+id/Pres_TS_Label"
        android:layout_alignEnd="@+id/Prev_TS_Value"
        android:layout_alignLeft="@+id/Prev_TS_Value"
        android:layout_alignRight="@+id/Prev_TS_Value"
        android:layout_alignStart="@+id/Prev_TS_Value"
        tools:layout_editor_absoluteY="109dp"
        tools:layout_editor_absoluteX="125dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="Address"
        android:text="Address"
        tools:layout_editor_absoluteY="173dp"
        tools:layout_editor_absoluteX="42dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="68dp"
        android:layout_below="@+id/Pres_TS_Label"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/Address_Value"
        android:layout_width="197dp"
        android:layout_height="16dp"
        android:layout_alignEnd="@+id/Pres_TS_Value"
        android:layout_alignLeft="@+id/Pres_TS_Value"
        android:layout_alignRight="@+id/Pres_TS_Value"
        android:layout_alignStart="@+id/Pres_TS_Value"
        android:layout_alignTop="@+id/textView5"
        tools:layout_editor_absoluteX="129dp"
        tools:layout_editor_absoluteY="174dp" />

    <TextView
        android:id="@+id/Timestamp_Label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="63dp"
        android:text="TimeStamp"
        android:layout_below="@+id/textView5"
        android:layout_toStartOf="@+id/Timestamp_Value" />

    <TextView
        android:id="@+id/Timestamp_Value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/Timestamp_Label"
        android:layout_alignEnd="@+id/Address_Value"
        android:layout_alignStart="@+id/Address_Value"
        android:layout_marginStart="10dp" />

    <TextView
        android:id="@+id/Time_Label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/Timestamp_Label"
        android:layout_below="@+id/Timestamp_Label"
        android:layout_marginTop="44dp"
        android:text="Time"
        android:layout_toStartOf="@+id/Prev_TS_Value" />

    <TextView
        android:id="@+id/Time_Value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/Time_Label"
        android:layout_alignEnd="@+id/Timestamp_Value"
        android:layout_alignStart="@+id/Timestamp_Value" />

</RelativeLayout>
